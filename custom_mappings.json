    "direction": "bidirectional",
    "conflict_resolution": "last_modified_wins",
    "complex_mappings": {
      "project": {
        "type": "lookup",
        "supabase_table": "projects",
        "frappe_doctype": "Project",
        "supabase_field": "id",
        "frappe_field": "name",
        "prefix_logic": "PROJ-0XX"
      },
      "type": {
        "type": "lookup",
        "frappe_doctype": "Task Type",
        "supabase_field": "task_category",
        "frappe_field": "name"
      },
      "_assign": {
        "type": "array_to_uuid",
        "description": "Convert Frappe assignment array to Supabase assigned_to UUID array"
      },
      "is_milestone": {
        "type": "boolean_mapping",
        "description": "Map Frappe is_milestone boolean to Supabase needs_submission boolean"
      }
    },
    "confidence_score": 0.95,
    "created_at": "2025-01-27T00:00:00Z"
  },
